{
  "ConnectionStrings": {
    "Postgres": " <- your db connection string here  -> "
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [ "Console" ]
  },
  "JwtSetting": {
    "SecurityKey": "<- your jwt security key here ->",
    "AccessTokenLifetime": "3600"
  },
  "AllowedHosts": "*"
}
